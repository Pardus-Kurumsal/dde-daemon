{
	"Actions": [
	{
		"Key": "launcher",
		"Action": "dbus-send --print-reply --dest=com.deepin.dde.Launcher /com/deepin/dde/Launcher com.deepin.dde.Launcher.Toggle"
	},
	{
		"Key": "terminal",
		"Action": "/usr/lib/deepin-daemon/default-terminal"
	},
	{
		"Key":"deepin-screen-recorder",
		"Action":"/usr/bin/deepin-screen-recorder"
	},
	{
		"Key": "lock-screen",
		"Action": "dbus-send --print-reply --dest=com.deepin.dde.lockFront /com/deepin/dde/lockFront com.deepin.dde.lockFront.Show"
	},
	{
		"Key": "show-dock",
		"Action": "dbus-send --type=method_call --dest=com.deepin.daemon.Dock /dde/dock/HideStateManager dde.dock.HideStateManager.ToggleShow"
	},
	{
		"Key": "logout",
		"Action": "dbus-send --print-reply --dest=com.deepin.dde.shutdownFront /com/deepin/dde/shutdownFront com.deepin.dde.shutdownFront.Show"
	},
	{
		"Key": "terminal-quake",
		"Action": "deepin-terminal --quake-mode"
	},
	{
		"Key": "screenshot",
		"Action": "deepin-screenshot"
	},
	{
		"Key": "screenshot-fullscreen",
		"Action": "deepin-screenshot -f"
	},
	{
		"Key": "screenshot-window",
		"Action": "deepin-screenshot -w"
	},
	{
		"Key": "screenshot-delayed",
		"Action": "deepin-screenshot -d 5"
	},
	{
		"Key": "file-manager",
		"Action": "/usr/lib/deepin-daemon/default-file-manager"
	},
	{
		"Key": "disable-touchpad",
		"Action": "gsettings set com.deepin.dde.touchpad touchpad-enabled false"
	},
	{
		"Key": "wm-switcher",
		"Action": "dbus-send --type=method_call --dest=com.deepin.wm_switcher /com/deepin/wm_switcher com.deepin.wm_switcher.requestSwitchWM"
	},
	{
		"Key": "turn-off-screen",
		"Action": "sleep 0.5; xset dpms force off"
	},
	{
		"Key": "switch-layout",
		"Action": ""
	}
	]
}
